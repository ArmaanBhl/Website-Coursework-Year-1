    <!--JS for my filter-->
    <script>
      function filterTable() {
        var dayInput, filter, table, tr, td; //get the input and its value
        dayInput = document.getElementById("dayFilter"); //gets the table and its rows
        filter = dayInput.value.toUpperCase();
        table = document.getElementById("filterTable");
        tr = table.getElementsByTagName("tr");

        for (i = 0; i < tr.length; i++) {
          //loops through each row of the table
          td = tr[i].getElementsByTagName("td")[0]; //gets the first element of the row
          if (td) {
            dayValue = td.textContent || td.innerText; //gets the content of the cell
            if (dayValue.toUpperCase().indexOf(filter) > -1) {
              //checks if the input is in the cell
              tr[i].style.display = ""; // if its found show the row
            } else {
              tr[i].style.display = "none"; // if not hide the row
            }
          }
        }
      }
    </script>

